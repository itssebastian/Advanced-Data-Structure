def search(arr, n, value): 
  
    for i in range (0, n): 
        if (arr[i] == value): 
            return i;
            
    return -1;

 
arr = [ 10,5,20,45,90,1];
n = len(arr);  
value = int(input("Enter the Value to search:")) 
result = search(arr, n, value) 
if(result == -1): 
    print("Element is not present in array") 
else: 
    print("Element is present at index",result)
