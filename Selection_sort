def SelectionSort(nums):
  for i in range(len(nums)):
    least = i
    for j in range(i + 1 , len(nums)):
      if nums[j] < nums[least]:
        least = j
 
    swap(nums, least, i)
 
 
def swap(nums, x, y):
  temp = nums[x]
  nums[x] = nums[y]
  nums[y] = temp
	      
      
nums = [54, 26, 93, 17, 77, 31, 44, 55, 20]
SelectionSort(nums)
print(nums) 
